	"type": "module",
	"packageManager": "pnpm@10.12.1",
	"source": "src/index.ts",
	"types": "dist/index.d.ts",
	"targets": {
		"main": {
			"source": "src/index.ts",
			"distDir": "dist",
			"isLibrary": true,
			"includeNodeModules": false

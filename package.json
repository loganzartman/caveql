		"parcel": "^2.15.4",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.11",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@headlessui/react": "^2.2.4",

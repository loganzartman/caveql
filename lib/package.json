{
  "name": "caveql",
  "version": "0.2.0",
  "private": false,
  "description": "",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/main/index.js",
  "types": "dist/main/index.d.ts",
  "bin": "dist/cli/cli.js",
  "targets": {
    "main": {
      "isLibrary": true,
      "source": "src/index.ts",
      "outputFormat": "esmodule"
    },
    "types": {
      "isLibrary": true,
      "source": "src/index.ts",
      "outputFormat": "esmodule"
    },
    "bin": {
      "isLibrary": true,
      "source": "src/cli/cli.ts",
      "outputFormat": "esmodule"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">= 18.0.0"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "scripts": {
    "dev": "parcel watch --port 1235",
    "build": "parcel build && chmod +x dist/cli/cli.js",
    "clean": "rm -rf dist .parcel-cache",
    "cli": "node dist/cli/cli.js"
  },
  "author": "Logan Zartman",
  "license": "MPL-2.0",
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "@parcel/bundler-library": "^2.16.0",
    "@types/node": "^24.0.6",
    "monaco-editor": "^0.52.2"
  },
  "dependencies": {
    "@streamparser/json-whatwg": "^0.0.22",
    "@stricli/core": "^1.2.0",
    "tinyqueue": "^3.0.0",
    "udsv": "^0.7.3",
    "zod": "^4.0.17"
  }
}

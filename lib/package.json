{
  "name": "caveql",
  "version": "0.2.0",
  "private": false,
  "description": "",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/main/index.js",
  "types": "dist/main/index.d.ts",
  "bin": "dist/cli/cli.js",
  "targets": {
    "main": {
      "isLibrary": true,
      "source": "src/index.ts",
      "outputFormat": "esmodule"
    },
    "types": {
      "isLibrary": true,
      "source": "src/index.ts",
      "outputFormat": "esmodule"
    },
    "bin": {
      "isLibrary": true,
      "source": "src/cli/cli.ts",
      "outputFormat": "esmodule"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">= 18.0.0"
  },
  "scripts": {
    "dev": "parcel watch --port 1235",
    "build": "parcel build && chmod +x dist/cli/cli.js",
    "clean": "rm -rf dist .parcel-cache",
    "cli": "node dist/cli/cli.js"
  },
  "author": "Logan Zartman",
  "license": "MPL-2.0",
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "@types/stream-chain": "^2.1.0",
    "@types/stream-csv-as-json": "^1.0.4",
    "@types/stream-json": "^1.7.8",
    "monaco-editor": "^0.52.2"
  },
  "dependencies": {
    "@stricli/core": "^1.2.0",
    "stream-chain": "^3.4.0",
    "stream-csv-as-json": "^1.0.5",
    "stream-json": "^1.9.1",
    "tinyqueue": "^3.0.0",
    "zod": "^4.0.17"
  }
}
